PROGRAM Ggt;
(* Berechnet den GGT zweier Zahlen *)
VAR
    z1, z2, z3[4], z4[4];
BEGIN
    (* Startwerte *)
    z3 := z4;
	z1 := 5555;
    z2 := 22755;
	(* eigentliche Berechnung *)
    WHILE z1 <> z2
    DO
        IF z1 >= z2
        THEN
            z1 := z1 - z2
        (*ELSE
            z2 := z2 - z1*)
        END
    END
END Ggt.
